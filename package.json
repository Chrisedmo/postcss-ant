{
  "name": "postcss-ant",
  "version": "1.0.0",
  "license": "MIT",
  "description": "If you're not using Grid Spec, use postcss-ant.",
  "author": "Cory Simmons <github.cory@mailhero.io> (https://corysimmons.com)",
  "repository": "corysimmons/postcss-ant",
  "main": "./index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "scripts": {
    "start": "npm-run-all --parallel babel demo",
    "babel": "babel -w lib/index.js -o index.js",
    "demo": "webpack-dev-server --config demo.config.js"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "postcss": "^5.2.5",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-react-stateless-component-name": "^1.0.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "lodash": "^4.16.4",
    "npm-run-all": "^3.1.1",
    "react": "^15.4.0-rc.4",
    "react-dom": "^15.4.0-rc.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
