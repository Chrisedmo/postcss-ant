{
  "name": "postcss-ant",
  "version": "0.1.0",
  "description": "A size-getting function that accepts fractions, decimals, fixed numbers, and everything in-between.",
  "license": "MIT",
  "author": {
    "name": "Cory Simmons"
  },
  "contributors": [
    {
      "name": "Neil Kistner",
      "email": "neil.kistner@gmail.com",
      "url": "https://neilkistner.com"
    }
  ],
  "scripts": {
    "start": "npm-run-all -p chokidar:babel chokidar:sass chokidar:postcss browser-sync",
    "chokidar:babel": "chokidar './lib/index.babel.js' -c 'npm run babel'",
    "xxx-chokidar:stylus": "chokidar './demo/css/src/in.styl' -c 'npm run stylus'",
    "chokidar:stylus": "chokidar './tinker/style.styl' -c 'npm run stylus'",
    "chokidar:sass": "chokidar './tinker/in.scss' -c 'npm run sass'",
    "xxx-chokidar:postcss": "chokidar './demo/css/src/out.css' './index.js' -c 'npm run postcss'",
    "chokidar:postcss": "chokidar './tinker/out.css' './index.js' -c 'npm run postcss:sass'",
    "babel": "babel './lib/index.babel.js' -o './index.js'",
    "xxx-stylus": "stylus './demo/css/src/in.styl' -o './demo/css/src/out.css'",
    "stylus": "stylus './tinker/style.styl'",
    "sass": "node-sass tinker/in.scss tinker/out.css",
    "xxx-postcss": "postcss --local-plugins -u './index.js' -o './demo/css/style.css' './demo/css/src/out.css'",
    "postcss": "npm-run-all -p postcss:*",
    "postcss:stylus": "postcss --local-plugins -u './index.js' -o './tinker/final-stylus.css' './tinker/style.css'",
    "postcss:sass": "postcss --local-plugins -u './index.js' -o './tinker/final.css' './tinker/out.css'",
    "xxx-browser-sync": "browser-sync start --startPath='./demo' --server --files='./demo/index.html, ./demo/css/style.css, ./index.js' --no-notify --no-open",
    "browser-sync": "browser-sync start --startPath='./tinker' --server --files='./tinker/index.html, ./tinker/final.css, ./tinker/final-stylus.css, ./index.js' --no-notify --no-open"
  },
  "keywords": [
    "postcss-plugin",
    "postcss",
    "ant",
    "size",
    "grid",
    "layout",
    "auto",
    "flexbox"
  ],
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "postcss": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.14.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.4.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "postcss-calc": "^5.3.0",
    "standard": "^7.1.2",
    "stylus": "^0.54.5"
  }
}
