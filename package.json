{
  "name": "postcss-ant",
  "description": "A size getting function capable of grabbing auto lengths.",
  "version": "0.0.7",
  "author": {
    "name": "Cory Simmons"
  },
  "contributors": [
    {
      "name": "Neil Kistner",
      "email": "neil.kistner@gmail.com",
      "url": "https://neilkistner.com"
    }
  ],
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p chokidar:* browser-sync",
    "chokidar:ant": "chokidar './lib/index.js' -c 'npm run babel'",
    "chokidar:demo": "chokidar './demo/css/in.scss' -c 'npm run postcss'",
    "babel": "babel './lib/index.js' -o './index.js'",
    "postcss": "postcss -u precss -p postcss-scss --local-plugins -u './index.js' -o './demo/css/out.css' './demo/css/in.scss'",
    "browser-sync": "browser-sync start --startPath='./demo' --server --files='./demo/index.html, ./demo/css/out.css, ./index.js' --no-notify --no-open"
  },
  "keywords": [
    "postcss",
    "ant",
    "size",
    "grid",
    "layout",
    "auto"
  ],
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "dependencies": {
    "postcss": "^5.1.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.14.0",
    "chokidar-cli": "^1.2.0",
    "npm-run-all": "^2.3.0",
    "postcss": "^5.1.2",
    "postcss-calc": "^5.3.0",
    "postcss-scss": "^0.1.9",
    "precss": "^1.4.0",
    "standard": "^7.1.2"
  }
}
